/* pages/market/market.wxss */
.container {
    width: 100vw;
    height: 100vh;
    overflow: scroll;
    overflow-x: scroll;
    overflow-y: hidden;
    /*解决ios上滑动不流畅*/
    -webkit-overflow-scrolling: touch;
    background-color: #f1f1f1;
}
.linebar {
    border-left: 5px solid #ff5800;
    font-size: 28rpx;
    background-color: #fff;
    color: #2d2d2d;
    height: 80rpx;
    margin-bottom: 4rpx;
}
.resource {
    text-indent: 2em;
    font-size: 26rpx;
    color: #2d2d2d;
    background-color: #fff;
    margin-bottom: 4rpx;
    padding: 0 20rpx;
}